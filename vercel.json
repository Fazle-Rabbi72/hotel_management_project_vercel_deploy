{
    "version": 2,
    "functions": {
      "api/*": {
        "runtime": "@vercel/python",
        "maxDuration": 10,
        "memory": 1024,
        "handler": "hotel_management_project.asgi.application"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }
  